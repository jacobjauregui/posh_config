{
	"blocks": [
	{
		"alignment": "left",
		"segments": [
		{
			"type": "os",
			"style": "diamond",
			"background": "#00ffff",
			"foreground": "#000000",
			"properties": {
				"windows": "\ue62a "
			},
			"template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
			"leading_diamond": "<#00ffff,transparent>\u256d\u2500</>\ue0b6"
		},
		{
			"type": "root",
			"background": "#1a1a1a",
			"foreground": "#ea2020",
			"properties": {
				"root_icon": "\uedcf"
			},
			"style": "diamond",
			"template": "<parentBackground>\ue0b0</> <#ff8000>\ueaf2</> "
		},
		{
			"type": "path",
			"style": "powerline",
			"powerline_symbol": "\ue0b0",
			"background": "#111111",
			"foreground": "#00ffaa",
			"properties": {
				"folder_icon": "\uf07c",
				"home_icon": "\udb84\udcb5",
				"folder_separator_icon": "/",
				"mapped_locations": {
					"C:/": "# C:",
					"C:/Users/Jacob/OneDrive": "<#74db78>\udb80\ude4c</> OneDrive ",
					"C:/Users/Jacob/OneDrive/Pictures": "<#74db78>\udb80\ude4f</> Pictures ",
					"C:/Users/Jacob/OneDrive/Documents": "<#74db78>\udb84\udee3</> Documents ",
					"C:/Users/Jacob/Downloads": "<#74db78>\udb80\ude4d</> Downloads ",
					"C:/Users/Jacob/AppData": "<#74db78>\udb84\udc7f</> AppData "
				},
				"style": "full"
			},
			"template": " {{ path .Path .Location  }} "
		},
		{
			"type": "git",
			"style": "powerline",
			"powerline_symbol": "\ue0b0\ue0b1",
			"foreground": "#000000",
			"background": "#000000",
			"background_templates": [
				"{{ if or (.Working.Changed) (.Staging.Changed) }}#00ffaa{{ end }}",
				"{{ if and (gt .Ahead 0) (gt .Behind 0) }}#859900{{ end }}",
				"{{ if gt .Ahead 0 }}#6C71C4{{ end }}",
				"{{ if gt .Behind 0 }}#6C71C4{{ end }}"
			],
			"template": " {{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
			"properties": {
				"branch_icon": "<b>îœ¥</b> ",
				"fetch_status": true,
				"fetch_upstream_icon": true
			}
		}
		],
		"type": "prompt"
	},
	{
		"alignment": "right",
		"filler": "<#8b8b8b,transparent>\u2500</>",
		"segments": [
		{
			"type": "time",
			"background": "#18191d",
			"foreground": "#5bb5d5",
			"style": "diamond",
			"invert_powerline": true,
			"leading_diamond": "\ue0b6",
			"template": " <#727272>\udb82\udd54</> {{ .CurrentDate | date .Format }} ",
			"trailing_diamond": "\ue0b4"
		}
		],
		"type": "prompt"
	},
	{
		"alignment": "left",
		"newline": true,
		"segments": [
		{
			"type": "text",
			"foreground": "#c2c3c1",
			"style": "plain",
			"template": "\u2570\u2500"
		},
		{
			"type": "status",
			"style": "plain",
			"foreground": "#74db78",
			"foreground_templates": [
				"{{ if gt .Code 0 }}#ef5350{{ end }}"
			],
			"properties": {
				"always_enabled": true
			},
			"template": "PS \ue683 "
		}
		],
		"type": "prompt"
	}
	],
	"version": 2
}
