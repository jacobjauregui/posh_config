{
	"palette": {
		"red": "#fe085b", 
		"green": "#5bfe08",
		"yellow": "#eaff08",
		"blue": "#085bfe",
		"orange": "#fe5b08",
		"cyan": "#08eafe",
		"grey": "#727272",
		"black": "#282828",
		"white": "#a9a9a9"
	},
	"blocks": [
	{
		"alignment": "left",
		"segments": [
		{
			"type": "os",
			"style": "diamond",
			"background": "p:grey",
			"foreground": "p:cyan",
			"properties": {
				"windows": "\ue62a "
			},
			"template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
			"leading_diamond": "<#c3c1c2,transparent>\u256d\u2500</>\ue0b6"
		},
		{
			"type": "root",
			"background": "#1a1a1a",
			"foreground": "#ea2020",
			"properties": {
				"root_icon": "\uedcf"
			},
			"style": "diamond",
			"template": "<parentBackground>\ue0b0</> <#ff8000>\ueaf2</> "
		},
		{
			"type": "path",
			"style": "powerline",
			"powerline_symbol": "\ue0b0\ue0b1",
			"background": "p:black",
			"foreground": "p:blue",
			"foreground_templates": [
				"{{ if .RootDir }}p:yellow{{ end }}",
				"{{ if not .Writable }}p:red{{ end }}"
			],
			"properties": {
				"folder_icon": "\uf07c",
				"folder_separator_icon": " <#a9a9a9>\uf0da</> ",
				"style": "agnoster_full",
				"mapped_locations_enabled": true,
				"mapped_locations": {
					"C:\\": "\udb82\ude07",
					"~/": "<#a9a9a9>\uf015</>  Home",
					"~/AppData": "<#a9a9a9>\ue615</>  AppData",
					"~/Downloads": "<#a9a9a9>\udb80\ude4d</>  Downloads",
					"~/OneDrive": "<#085bff>\udb80\udd5f</>  OneDrive",
					"~/OneDrive/Documents": "<#a9a9a9>\udb84\udee3</>  Documents",
					"~/OneDrive/Files": "<#a9a9a9>\udb84\udee3</>  Files",
					"~/OneDrive/Pictures": "<#a9a9a9>\udb80\ude4f</>  Pictures"
				}
			},
			"template": " {{ .Path }} "
		},
		{
			"type": "git",
			"style": "powerline",
			"powerline_symbol": "\ue0b0\ue0b1",
			"foreground": "p:black",
			"background": "#000000",
			"background_templates": [
				"{{ if or (.Working.Changed) (.Staging.Changed) }}p:white{{ end }}",
				"{{ if and (gt .Ahead 0) (gt .Behind 0) }}#859900{{ end }}",
				"{{ if gt .Ahead 0 }}#6C71C4{{ end }}",
				"{{ if gt .Behind 0 }}#6C71C4{{ end }}"
			],
			"template": " \ueba1 {{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
			"properties": {
				"branch_icon": " îœ¥ ",
				"fetch_status": true,
				"fetch_upstream_icon": true
			}
		}
		],
		"type": "prompt"
	},
	{
		"alignment": "right",
		"filler": "<#939393,transparent>\u2500</>",
		"segments": [
		{
			"type": "time",
			"background": "#353535",
			"foreground": "p:green",
			"style": "diamond",
			"invert_powerline": true,
			"leading_diamond": "\ue0b6",
			"template": " <#8bff18>\udb82\udd54</> {{ .CurrentDate | date .Format }} ",
			"trailing_diamond": "\ue0b4"
		}
		],
		"type": "prompt"
	},
	{
		"alignment": "left",
		"newline": true,
		"segments": [
		{
			"type": "text",
			"foreground": "#c2c3c1",
			"style": "plain",
			"template": "\u2570\u2500"
		},
		{
			"type": "status",
			"style": "plain",
			"foreground": "p:green",
			"foreground_templates": [
				"{{ if gt .Code 0 }}p:red{{ end }}"
			],
			"properties": {
				"always_enabled": true
			},
			"template": "<b><i><#085bff>PS</></i></b> \ue683 "
		}
		],
		"type": "prompt"
	}
	],
	"version": 2
}
